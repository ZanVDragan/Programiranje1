import java.util.Scanner;

class PostevankaI {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        //Preberi vrednosti.
        int a = sc.nextInt();
        int b = sc.nextInt();
        
        //Nastavi vrednost faktorja i in preveri pogoj za b.
        //Povecaj faktor i.
        for (int i = 1; i <= b; i++){
            //Zmnozi a in faktor i.
            int rezultat = a * i;
            //Izpis.
            System.out.printf("%d * %d = %d%n", a, i, rezultat);
        }
    }
}
